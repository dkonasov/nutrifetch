  "devDependencies": {
    "@kubb/cli": "^3.6.4",
    "@kubb/core": "^3.6.4",
    "@kubb/plugin-oas": "^3.6.4",
    "@kubb/plugin-ts": "^3.6.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-oxlint": "^0.15.13",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.13",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
